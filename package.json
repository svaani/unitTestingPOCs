{
  "devDependencies": {
    "ava": "^5.2.0",
    "jasmine": "^4.5.0",
    "mocha": "^10.2.0",
    "nock": "^13.3.0",
    "sinon": "^15.0.1",
    "vitest": "^0.28.5"
  },
  "type": "module",
  "scripts": {
    "test-jasmine": "npx jasmine --config=1_jasmine/jasmine.json",
    "test-jasmine-refactored": "npx jasmine --config=2_jasmine_refactored/jasmine.json",
    "test-mocha": "mocha --recursive 2_mocha/source/**/__tests/**.js",
    "test-mocha-sinon": "mocha --recursive 3_mocha_sinon/source/**/__tests/**.js",
    "test-nock": "mocha --recursive 4_nock/source/**/__tests/**.js",
    "test-ava": "ava -r esm 5_ava/source/**/__tests/**.js",
    "test-tape": "tape 6_tape/source/**/__tests/**.js",
    "test-vitest": "vitest --config 7_vitest/vitest.config.js"
  },
  "name": "unittestingpocs",
  "version": "1.0.0",
  "main": "jasmine_transpiler.js",
  "dependencies": {
    "axios": "^1.3.4",
    "tape": "^5.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/vanishivanand/unittestingpocs.git"
  },
